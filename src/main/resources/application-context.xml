<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
 
  <bean id="prototype" class="com.scope.Prototype" scope="prototype">
  <property name="message" value="prototype"></property>
  </bean>
  
   <bean id="singleton" class="com.scope.Singleton" scope="singleton">
 		<property name="prototype" ref="prototype"></property>
 		<property name="message" value="singleton"></property>
 </bean>
  
</beans>